{"ast":null,"code":"import ButtonGroup from '@/components/ButtonGroup.vue';\nimport { imageUrls } from '@/imageLoader';\nexport default {\n  components: {\n    ButtonGroup\n  },\n  data() {\n    return {\n      currentIndex: 0,\n      images: imageUrls,\n      categories: [/* ... */],\n      selectedCategory: ''\n    };\n  },\n  computed: {\n    currentImage() {\n      return this.images[this.currentIndex];\n    }\n  },\n  methods: {\n    selectCategory(category) {\n      this.selectedCategory = category;\n    },\n    nextImage() {\n      // Logic to move to the next image\n      if (this.currentIndex < this.images.length - 1) {\n        this.currentIndex++;\n        this.selectedCategory = '';\n      } else {\n        this.$emit('exercise-complete');\n      }\n    }\n  }\n};","map":{"version":3,"names":["ButtonGroup","imageUrls","components","data","currentIndex","images","categories","selectedCategory","computed","currentImage","methods","selectCategory","category","nextImage","length","$emit"],"sources":["/Users/stanley/Documents/Projecten/Archief/2023/mariendael-kunstquiz/kunstquiz/interactive-image-categorization/src/components/ExerciseScreen1.vue"],"sourcesContent":["<template>\n  <div class=\"exercise\">\n    <!-- Display image -->\n    <img :src=\"currentImage\" alt=\"Current Image\" />\n\n    <!-- Use ButtonGroup component for categories -->\n    <button-group :buttons=\"categories\" @button-click=\"selectCategory\" />\n\n    <!-- Display progress tracker -->\n    <p>{{ currentIndex + 1 }} / {{ images.length }}</p>\n\n    <!-- Advance to the next image -->\n    <button @click=\"nextImage\">Next</button>\n  </div>\n</template>\n\n<script>\nimport ButtonGroup from '@/components/ButtonGroup.vue';\nimport { imageUrls } from '@/imageLoader';\n\nexport default {\n  components: {\n    ButtonGroup,\n  },\n  data() {\n    return {\n      currentIndex: 0,\n      images: imageUrls,\n      categories: [/* ... */],\n      selectedCategory: '',\n    };\n  },\n  computed: {\n    currentImage() {\n      return this.images[this.currentIndex];\n    },\n  },\n  methods: {\n    selectCategory(category) {\n      this.selectedCategory = category;\n    },\n    nextImage() {\n      // Logic to move to the next image\n      if (this.currentIndex < this.images.length - 1) {\n        this.currentIndex++;\n        this.selectedCategory = '';\n      } else {\n        this.$emit('exercise-complete');\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Add your component-specific styling here */\n</style>\n"],"mappings":"AAiBA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,SAASC,SAAQ,QAAS,eAAe;AAEzC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC;MACfC,MAAM,EAAEJ,SAAS;MACjBK,UAAU,EAAE,CAAC,UAAU;MACvBC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACJ,MAAM,CAAC,IAAI,CAACD,YAAY,CAAC;IACvC;EACF,CAAC;EACDM,OAAO,EAAE;IACPC,cAAcA,CAACC,QAAQ,EAAE;MACvB,IAAI,CAACL,gBAAe,GAAIK,QAAQ;IAClC,CAAC;IACDC,SAASA,CAAA,EAAG;MACV;MACA,IAAI,IAAI,CAACT,YAAW,GAAI,IAAI,CAACC,MAAM,CAACS,MAAK,GAAI,CAAC,EAAE;QAC9C,IAAI,CAACV,YAAY,EAAE;QACnB,IAAI,CAACG,gBAAe,GAAI,EAAE;MAC5B,OAAO;QACL,IAAI,CAACQ,KAAK,CAAC,mBAAmB,CAAC;MACjC;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}