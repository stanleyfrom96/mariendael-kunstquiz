{"ast":null,"code":"import ButtonGroup from '@/components/ButtonGroup.vue';\nimport { imageUrls } from '@/imageLoader';\nexport default {\n  components: {\n    ButtonGroup\n  },\n  props: {\n    secondOptions: Array // Passed from App.vue\n  },\n\n  data() {\n    return {\n      currentIndex: 0,\n      images: imageUrls.map(url => ({\n        url,\n        exercise1Category: null,\n        // Original category assigned in the first exercise\n        newCategory: null // Updated category during recategorization\n      }))\n    };\n  },\n\n  computed: {\n    currentImage() {\n      return this.images[this.currentIndex];\n    }\n  },\n  methods: {\n    recategorizeImage(category) {\n      this.currentImage.newCategory = category;\n      if (this.currentIndex < this.images.length - 1) {\n        this.currentIndex++;\n      } else {\n        this.$emit('exercise-complete', this.images); // Emit the categorized images\n      }\n    }\n  }\n};","map":{"version":3,"names":["ButtonGroup","imageUrls","components","props","secondOptions","Array","data","currentIndex","images","map","url","exercise1Category","newCategory","computed","currentImage","methods","recategorizeImage","category","length","$emit"],"sources":["/Users/stanley/Documents/Projecten/Archief/2023/mariendael-kunstquiz/kunstquiz/interactive-image-categorization/src/components/ExerciseScreen3.vue"],"sourcesContent":["<template>\n  <div class=\"exercise\">\n    <!-- Display image -->\n    <img :src=\"currentImage.url\" alt=\"Current Image\" />\n\n    <!-- Use ButtonGroup component for recategorization -->\n    <button-group :buttons=\"recategorizeCategories\" @button-click=\"recategorizeImage\" />\n\n    <!-- Display progress tracker -->\n    <p>{{ currentIndex + 1 }} / {{ images.length }}</p>\n  </div>\n</template>\n\n<script>\nimport ButtonGroup from '@/components/ButtonGroup.vue';\nimport { imageUrls } from '@/imageLoader';\n\nexport default {\n  components: {\n    ButtonGroup,\n  },\n  props: {\n    secondOptions: Array, // Passed from App.vue\n  },\n  data() {\n    return {\n      currentIndex: 0,\n      images: imageUrls.map(url => ({\n        url,\n        exercise1Category: null, // Original category assigned in the first exercise\n        newCategory: null, // Updated category during recategorization\n      })),\n    };\n  },\n  computed: {\n    currentImage() {\n      return this.images[this.currentIndex];\n    },\n  },\n  methods: {\n    recategorizeImage(category) {\n      this.currentImage.newCategory = category;\n\n      if (this.currentIndex < this.images.length - 1) {\n        this.currentIndex++;\n      } else {\n        this.$emit('exercise-complete', this.images); // Emit the categorized images\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Add your component-specific styling here */\n</style>\n"],"mappings":"AAcA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,SAASC,SAAQ,QAAS,eAAe;AAEzC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,aAAa,EAAEC,KAAK,CAAE;EACxB,CAAC;;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC;MACfC,MAAM,EAAEP,SAAS,CAACQ,GAAG,CAACC,GAAE,KAAM;QAC5BA,GAAG;QACHC,iBAAiB,EAAE,IAAI;QAAE;QACzBC,WAAW,EAAE,IAAI,CAAE;MACrB,CAAC,CAAC;IACJ,CAAC;EACH,CAAC;;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACN,MAAM,CAAC,IAAI,CAACD,YAAY,CAAC;IACvC;EACF,CAAC;EACDQ,OAAO,EAAE;IACPC,iBAAiBA,CAACC,QAAQ,EAAE;MAC1B,IAAI,CAACH,YAAY,CAACF,WAAU,GAAIK,QAAQ;MAExC,IAAI,IAAI,CAACV,YAAW,GAAI,IAAI,CAACC,MAAM,CAACU,MAAK,GAAI,CAAC,EAAE;QAC9C,IAAI,CAACX,YAAY,EAAE;MACrB,OAAO;QACL,IAAI,CAACY,KAAK,CAAC,mBAAmB,EAAE,IAAI,CAACX,MAAM,CAAC,EAAE;MAChD;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}