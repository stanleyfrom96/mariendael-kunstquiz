urn this.images[this.currentIndex];\n    },\n  },\n  methods: {\n    selectCategoryAndAdvance(category) {\n      this.selectedCategory = category;\n      if (this.currentIndex < this.images.length - 1) {\n        this.currentIndex++;\n        this.selectedCategory = '';\n      } else {\n        this.$emit('exercise-complete', this.selectedCategory);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Add your component-specific styling here */\n</style>\n"],"mappings":"AAcA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,SAASC,SAAQ,QAAS,eAAe;AAEzC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,UAAU,EAAEC,KAAK,CAAE;EACrB,CAAC;;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC;MACfC,MAAM,EAAEP,SAAS;MACjBQ,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACH,MAAM,CAAC,IAAI,CAACD,YAAY,CAAC;IACvC;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,wBAAwBA,CAACC,QAAQ,EAAE;MACjC,IAAI,CAACL,gBAAe,GAAIK,QAAQ;MAChC,IAAI,IAAI,CAACP,YAAW,GAAI,IAAI,CAACC,MAAM,CAACO,MAAK,GAAI,CAAC,EAAE;QAC9C,IAAI,CAACR,YAAY,EAAE;QACnB,IAAI,CAACE,gBAAe,GAAI,EAAE;MAC5B,OAAO;QACL,IAAI,CAACO,KAAK,CAAC,mBAAmB,EAAE,IAAI,CAACP,gBAAgB,CAAC;MACxD;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}